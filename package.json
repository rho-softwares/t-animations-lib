{
  "name": "t-animations-lib",
  "version": "0.1.22",
  "description": "Fastest Declarative CSS Animations tool",
  "bin": "dst/t-animations.js",
  "scripts": {
    "bin": "dst/t-animations.js",
    "patch-push": "npm version patch && git push rho-softwares main",
    "lint": "npx clj-kondo --lint src",
    "style-fix": "cljfmt src/t_animations/core.cljs  src/t_animations/generator.cljs src/t_animations/utils.cljs",
    "release-cljs": "cd ../t-animations-lib && npx shadow-cljs release t-animations",
    "watch-cljs": "cd ../t-animations-lib && npx shadow-cljs watch t-animations"
  },
  "keywords": [
    "declarative",
    "css",
    "animations",
    "cli",
    "flexiana",
    "rho-softwares",
    "t-animations",
    "tailwind-css",
    "tachyons-css",
    "cljs",
    "shadow-cljs"
  ],
  "author": "RhoSoftwares@Flexiana",
  "license": "MIT",
  "dependencies": {}
}
